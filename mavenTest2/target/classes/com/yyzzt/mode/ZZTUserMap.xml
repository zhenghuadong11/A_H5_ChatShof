<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping package="com.yyzzt.mode">
    <class name="ZZTUserMap" table="c_userMap">
        <id name="id" type="int" column="id">
            <generator class="native"/>
        </id>
        <property name="user" type="string" column="user" />
        
        <property name="friendUser" type="string" column="friendUser" />
        <property name="type" type="byte" column="type" />

        
       
    </class>
</hibernate-mapping>